{
  "coc.preferences.timeout": 500,
  "coc.preferences.acceptSuggestionOnCommitCharacter": true,
  "coc.preferences.autoTrigger": "always",
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": ["javascriptreact", "javascript", "postcss", "json", "html", "vue", "css", "svg"],
  "coc.preferences.hoverTarget": "float",
  "codeLens.enable": true,
  "signature.target": "float",
  "suggest.echodocSupport": true,
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 32,
  "suggest.noselect": false,
  "suggest.timeout": 300,
  "diagnostic.displayByAle": true,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯦ",
  "diagnostic.maxWindowWidth": 120,
  "diagnostic.signPriority": 20,
  "eslint.autoFixOnSave": true,
  "prettier.eslintIntegration": true,
  "prettier.formatterPriority": -1,
  "tailwindCSS.headwind.removeDuplicates": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.headwind.sortTailwindClasses": true,
  "tailwindCSS.cssLanguages": ["css", "vue", "postcss"],
  "tailwindCSS.htmlLanguages": ["html", "vue"],
  "tailwindCSS.jsLanguages": ["javascript"],
  "vetur.completion.autoImport": false
}

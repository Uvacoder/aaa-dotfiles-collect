{
  "diagnostic.displayByAle": false,
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascriptreact",
    "javascript",
    "postcss",
    "json",
    "html",
    "vue",
    "svg"
  ],
  "codeLens.enable": true,
  "signature.target": "float",
  "suggest.echodocSupport": true,
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 32,
  "suggest.noselect": false,
  "suggest.timeout": 500,
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/utils/snips"],
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "▲",
  "diagnostic.infoSign": "■",
  "diagnostic.hintSign": "◆",
  "diagnostic.maxWindowWidth": 120,
  "diagnostic.signPriority": 20,
  "diagnostic.virtualText": true,
  "tailwindCSS.cssLanguages": ["css", "vue", "postcss"],
  "tailwindCSS.htmlLanguages": ["html", "vue"],
  "tailwindCSS.jsLanguages": ["javascript"],
  "tailwindCSS.headwind.removeDuplicates": true,
  "tailwindCSS.headwind.runOnSave": false,
  "vetur.completion.autoImport": false,
  "eslint.autoFixOnSave": true,
  "prettier.eslintIntegration": true
}
